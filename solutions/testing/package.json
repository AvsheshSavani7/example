{
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test-server": "next build && next start",
    "integration": "TEST_TYPE=integration playwright test --config playwright/playwright.config.ts",
    "integration-chrome": "PAUSE_ON_FAILURE=true npm run integration -- --project=chromium",
    "integration-debug": "PWDEBUG=1 npm run integration-chrome",
    "integration-watch": "chokidar 'playwright/integration/tests/**/*.spec.ts' -c 'npm run integration-chrome -- --headed {path}'",
    "e2e": "TEST_TYPE=e2e playwright test --config playwright/playwright.config.ts",
    "e2e-chrome": "PAUSE_ON_FAILURE=true npm run e2e -- --project=chromium",
    "e2e-debug": "PWDEBUG=1 npm run e2e-chrome",
    "e2e-watch": "chokidar 'playwright/e2e/tests/**/*.spec.ts' -c 'npm run e2e-chrome -- --headed {path}'"
  },
  "dependencies": {
    "@vercel/examples-ui": "latest",
    "clsx": "^1.2.1",
    "cookie": "^0.5.0",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "sluga": "^0.2.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.23.2",
    "@types/cookie": "^0.5.1",
    "@types/node": "^17",
    "@types/react": "latest",
    "autoprefixer": "^10.4.7",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.3"
  }
}
